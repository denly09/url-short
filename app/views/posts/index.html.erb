<div class="container ">
  <h1>Url list</h1>
</div>

<div class="container">
  <table class="table table-bordered table-info table-striped">
    <thead>
  <td>Url</td>
    <td>E-mail</td>
  </thead>

  <% @posts.each do |post| %>
    <tr>
      <td><%= post.url %></td>
      <td><%= post.user.email %></td>
    </tr>
  <% end %>
</table>
</div>
